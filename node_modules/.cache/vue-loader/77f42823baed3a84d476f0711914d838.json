":"/Users/mona/Desktop/main-prj-09-filtering-coaches/src/pages/requests/RequestsReceived.vue","sourceRoot":"","sourcesContent":["<template>\n  <section>\n    <base-dialog :v-show=\"!!error\" title=\"An error occoured\" @close=\"handleError\">\n      <p>{{error}}</p>\n    </base-dialog>\n    <base-card>\n      <header>\n        <h2>Requests Received</h2>\n      </header>\n      <div v-if=\"isloading\">\n        <base-spinner></base-spinner>\n      </div>\n      <ul v-else-if=\"hasRequests && !isloading\">\n        <request-item\n          v-for=\"req in receivedRequests\"\n          :key=\"req.id\"\n          :email=\"req.userEmail\"\n          :message=\"req.message\"\n        ></request-item>\n      </ul>\n      <h3 v-else>You haven't received any requests yet!</h3>\n    </base-card>\n  </section>\n</template>\n\n<script>\nimport RequestItem from '../../components/requests/RequestItem.vue';\n\nexport default {\n  data() {\n    return {\n      isLoading: false,\n      error: null\n    };\n  },\n  components: {\n    RequestItem\n  },\n  computed: {\n    receivedRequests() {\n      return this.$store.getters['requests/requests'];\n    },\n    hasRequests() {\n      return this.$store.getters['requests/hasRequests'];\n    }\n  },\n  created() {\n    this.loadRequests;\n  },\n  methods: {\n    async loadRequests() {\n      this.isLoading = true;\n      try {\n        await this.$store.dispatch('requeests/fetchRequest');\n      } catch (error) {\n        this.error = error.message || `Somthing Failed`;\n      }\n      this.isLoading = false;\n    },\n    handleError() {\n      this.error = null;\n    }\n  }\n};\n</script>\n\n<style scoped>\nheader {\n  text-align: center;\n}\n\nul {\n  list-style: none;\n  margin: 2rem auto;\n  padding: 0;\n  max-width: 30rem;\n}\n\nh3 {\n  text-align: center;\n}\n</style>"]}]}